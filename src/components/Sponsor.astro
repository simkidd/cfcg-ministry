---
import Sponsor1 from "../assets/images/sponsor/01.png"
import Sponsor2 from "../assets/images/sponsor/02.png"
import Sponsor3 from "../assets/images/sponsor/03.png"
import Sponsor4 from "../assets/images/sponsor/04.png"
---

<div class="sponsor">
  <div class="container">
    <div class="sponsor__slider overflow-hidden">
      <div class="swiper-wrapper">
        <div class="swiper-slide">
          <div class="sponsor__item">
            <div class="sponsor__inner">
              <div class="sponsor__thumb">
                <img src={Sponsor1.src} alt="sponsor thumb" />
              </div>
            </div>
          </div>
        </div>
        <div class="swiper-slide">
          <div class="sponsor__item">
            <div class="sponsor__inner">
              <div class="sponsor__thumb">
                <img src={Sponsor2.src} alt="sponsor thumb" />
              </div>
            </div>
          </div>
        </div>
        <div class="swiper-slide">
          <div class="sponsor__item">
            <div class="sponsor__inner">
              <div class="sponsor__thumb">
                <img src={Sponsor3.src} alt="sponsor thumb" />
              </div>
            </div>
          </div>
        </div>
        <div class="swiper-slide">
          <div class="sponsor__item">
            <div class="sponsor__inner">
              <div class="sponsor__thumb">
                <img src={Sponsor4.src} alt="sponsor thumb" />
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

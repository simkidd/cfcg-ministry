---
import About from "../components/About.astro";
import Banner from "../components/Banner.astro";
import Contact from "../components/Contact.astro";
import Preloader from "../components/Preloader.astro";
import Quote from "../components/Quote.astro";
import Sponsor from "../components/Sponsor.astro";
import UpcomingEvent from "../components/UpcomingEvent.astro";
import MainLayout from "../layouts/MainLayout.astro";
import { SITE_TITLE, SITE_DESCRIPTION } from "../utils/constants.ts";
---

<MainLayout title={SITE_TITLE} description={SITE_DESCRIPTION}>
  <Preloader />
  <Banner />
  <UpcomingEvent />
  <About />
  <Quote />
  <Sponsor />
  <Contact />
</MainLayout>
